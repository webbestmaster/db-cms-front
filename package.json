    "check": "npm run eslint && npm run stylelint && npm run tsc",
    "check:all": "npm run prettier; npm run eslint:fix; npm run stylelint:fix; npm run tsc",
    "check:fix": "npm run prettier && npm run eslint:fix && npm run stylelint:fix && npm run tsc",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint ./**.scss",
    "stylelint:fix": "stylelint ./**.scss --fix",
    "prettier": "prettier --write \"./**/*.(js|ts|tsx)\"",
    "tsc": "tsc -p ./tsconfig.json",
    "ts-coverage": "typescript-coverage-report"
  },
